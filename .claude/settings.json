{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "linkedin": {
      "command": "npx",
      "args": [
        "-y",
        "linkedin-mcp-server"
      ],
      "env": {
        "LINKEDIN_EMAIL": "op://Private/LinkedIn/username",
        "LINKEDIN_PASSWORD": "op://Personal/LinkedIn/password"
      }
    }
  }
}